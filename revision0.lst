0000148A                            NemDec_ProcessCompressedData:
0000148A 3E06                       		move.w	d6,d7
0000148C 5147                       		subq.w	#8,d7	; get shift value
0000148E 3205                       		move.w	d5,d1
00001490 EE69                       		lsr.w	d7,d1	; shift so that high bit of the code is in bit position 7
00001492 0C01 00FC                  		cmpi.b	#%11111100,d1	; are the high 6 bits set?
00001496 6400                       		bcc.s	NemPCD_InlineData	; if they are, it signifies inline data
00001498 0241 00FF                  		andi.w	#$FF,d1
0000149C D241                       		add.w	d1,d1
0000149E 1031 1000                  		move.b	(a1,d1.w),d0	; get the length of the code in bits
000014A2 4880                       		ext.w	d0
000014A4 9C40                       		sub.w	d0,d6	; subtract from shift value so that the next code is read next time around
000014A6 0C46 0009                  		cmpi.w	#9,d6	; does a new byte need to be read?
000014AA 6400                       		bcc.s	loc_14B2	; if not, branch
000014AC 5046                       		addq.w	#8,d6
000014AE E145                       		asl.w	#8,d5
